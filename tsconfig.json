{
  "extends": "@tsconfig/react-native/tsconfig.json",

  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@assets/*": ["./src/assets/*"],
      "@images/*": ["./src/assets/images/*"],
      "@common/*": ["./src/common/*"],
      "@components/*": ["./src/components/*"],
      "@hooks/*": ["./src/hooks/*"],
      "@layouts/*": ["./src/layouts/*"],
      "@redux": ["./src/redux"],
      "@redux/*": ["./src/redux/*"],
      "@routes": ["./src/routes"],
      "@routes/*": ["./src/routes/*"],
      "@icons": ["./src/icons"],
      "@screens/*": ["./src/screens/*"],
      "@services": ["./src/services"],
      "@services/*": ["./src/services/*"],
      "@typings": ["./src/typings"],
      "@designs/*": ["./src/designs/*"],
      "@navigator": ["src/navigator"],
      "@fragments": ["./src/fragments"],
      "@apiCaller": ["./src/apiCaller.ts"],
      "@themes": ["./src/themes"]
    }
  }
}
